<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias!</h1>

		<table>
			<tr>
				<td>ID</td>
				<td>Title</td>
				<td>News</td>
			</tr>

			<% news.forEach(function(notice) { %>
				<tr>
					<td> <%= notice.id %> </td>
					<td> <%= notice.title %> </td>
					<td> <%= notice.news %> </td>
				</tr>
			<% }); %>

		</table>

	</body>
</html>